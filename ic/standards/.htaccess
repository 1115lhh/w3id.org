Header set Access-Control-Allow-Origin *
Header set Access-Control-Allow-Headers DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified$
Options +FollowSymLinks
RewriteEngine on

#ISMCAT rewrite
RewriteRule ^ismcat/?$ https://intelshare.intelink.gov/sites/odni/cio/ea/library/Data%20Specifications/ism-cat/default.aspx [NC,R=302,L]

#Public Website rewrite
RewriteRule ^public/?$  http://www.dni.gov/index.php/about/organization/chief-information-officer/ic-cio-enterprise-integration-architecture [NC,R=302,L]
