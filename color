        <skos:altLabel xml:lang="zh">图像的粗糙度</skos:altLabel>
    </owl:Class>
    


    <!-- http://example.org/color-ontology#ImageContrast -->

    <owl:Class rdf:about="http://example.org/color-ontology#ImageContrast">
