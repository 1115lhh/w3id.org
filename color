        <rdfs:subClassOf rdf:resource="http://example.org/color-ontology#Application"/>
        <rdfs:label xml:lang="en">data type</rdfs:label>
        <skos:altLabel xml:lang="zh">数据类型</skos:altLabel>
    </owl:Class>
    


    <!-- http://example.org/color-ontology#Daylight -->

    <owl:Class rdf:about="http://example.org/color-ontology#Daylight">
        <rdfs:subClassOf rdf:resource="http://example.org/color-ontology#NaturalLightSource"/>
        <rdfs:comment xml:lang="zh">日光是自然光源中最常见的一种。在白天阳光明媚的环境中，颜色会看起来更鲜艳、明亮和自然。日光中的色温较高，偏向蓝色，这可能会使暖色调的颜色看起来较为冷静。</rdfs:comment>
        <rdfs:label xml:lang="en">day light</rdfs:label>
        <skos:altLabel xml:lang="zh">日光</skos:altLabel>
    </owl:Class>
    


