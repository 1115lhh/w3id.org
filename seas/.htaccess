Header set Access-Control-Allow-Origin *
Options +FollowSymLinks
RewriteEngine on

RewriteRule ^/green-button/?$ http://ci.emse.fr/green-button/ [R=302,L]
RewriteRule ^/green-button/(.+)$ http://ci.emse.fr/green-button/$1 [R=302,L]

RewriteRule ^/?$ http://ci.emse.fr/seas/ [R=302,L]
RewriteRule ^(.+)$ http://ci.emse.fr/seas/$1 [R=302,L]

